#Write a function that claculates the number of upper and lower case letters
def up_lows(s):
    lowcounter=0
    upcounter=0
    for x in s:
        if x.isupper():
            upcounter+=1
        if x.islower():
            lowcounter+=1
    print('OG string: '+ s)
    print(lowcounter)
    print(upcounter)
                
    
        
s='Hello Mr. Rogers, how are you this fine Tuesday?'
up_lows(s)
